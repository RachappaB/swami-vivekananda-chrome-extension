{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "permissions": [
      "tabs"
    ],
    "background.service_worker": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "chrome_url_overrides" : {
      "newtab": "index.html"
    },
    "action":{
      "default_popup": "1.html"
  }
  }